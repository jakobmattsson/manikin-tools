{
  "name": "manikin-tools",
  "description": "Utility functions for the manikin database interface",
  "version": "0.1.3",
  "main": "./lib/manikin-tools.js",
  "author": "Jakob Mattsson <jakob.mattsson@gmail.com> (jakobmattsson.se)",
  "license" : "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/jakobmattsson/manikin-tools.git"
  },
  "engines": {
    "node": ">= 0.8 < 0.12",
    "npm": ">= 1.3 < 2"
  },
  "dependencies": {
    "underscore": "^1.4.3"
  },
  "devDependencies": {
    "jscov": "^0.5.7",
    "mocha-term-cov-reporter": "^0.1.3",
    "mocha": "^1.7.4",
    "coffee-script": "^1.4.0",
    "should": "^1.2.1"
  },
  "scripts": {
    "pretest": "coffee -co lib src",
    "prepublish": "coffee -co lib src",
    "test": "mocha --compilers coffee:coffee-script spec/*.coffee",
    "coverage": "jscov src .cov && JSCOV=.cov mocha --compilers coffee:coffee-script --reporter mocha-term-cov-reporter spec/*",
    "prestart": "coffee -co lib src"
  }
}
